{
  "name": "revolution/botman-chatwork",
  "description": "BotMan driver for ChatWork",
  "keywords": [
    "botman",
    "chatwork",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "ext-json": "*",
    "botman/botman": "^2.0",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/support": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\BotMan\\Drivers\\ChatWork\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\BotMan\\Drivers\\ChatWork\\Providers\\ChatWorkDriverServiceProvider"
      ]
    }
  }
}
